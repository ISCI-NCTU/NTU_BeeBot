<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="simplebot">

  <material name="blue">
     <color rgba="0 0.2 0.4 1"/>
  </material>

<link name="base_link">
  <visual>
    <geometry>
      <cylinder length="2" radius="0.5"/>
    </geometry>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <material name="blue"/>
  </visual>

  <collision>
    <geometry>
      
      <cylinder length="2" radius="0.5"/>
    </geometry>
  <origin rpy="0 0 0" xyz="0 0 0"/>
  </collision>

  <inertial>
    <mass value="1"/>
    <origin pry="0 0 0" xyz="0 0 0"/>
    <inertia ixx="1.0" ixy="0" ixz="0"
             iyy="1.0" iyz="0" 
             izz="1.0"/>
  </inertial>

</link>
</robot>
