<launch>
<!--These will fire up two necessary components for the simulation.-->

  <include file="$(find simulation_tobot)/launch/nav/simulation_tobot.launch">
    <arg name="robot_model" value="$(find simulation_tobot)/URDF/beebot.urdf.xacro"/>
  </include>
  
  <!--Launch Rviz-->
  <node pkg="rviz" type="rviz" name="rviz"/>
</launch>
